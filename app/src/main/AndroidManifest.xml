<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aiyouwai.aphotoalbum">

    <application
        android:name=".base.AywApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.PersonActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.AlbumCreateActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.AlbumInfoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.AlbumApplyActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>